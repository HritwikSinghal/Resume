    "next": "^15.5.2",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.3.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.91.0",
